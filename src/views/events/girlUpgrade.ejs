<h2><%= __("event_girlUpgrade") %></h2>
<% if ( event.girlUpgrade.softCurrency ) { %>
    <%= formatNumber(event.girlUpgrade.softCurrency) %><%= __("unit_softCurrency") %><br />
<% } %>
<% if ( event.girlUpgrade.hardCurrency) { %>
    <%= formatNumber(event.girlUpgrade.hardCurrency) %> <%= __("unit_hardCurrency") %><br />
<% } %>
<% for ( const girl of event.girlUpgrade.girl ) { %>
    <div class="girl-ico">
        <img src="https://hh.hh-content.com/pictures/girls/<%= girl %>/ico0.png" />
    </div>
<% } %>
